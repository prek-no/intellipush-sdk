{
  "name": "@prek-no/intellipush-sdk",
  "version": "1.5.0",
  "author": "Chris Magnussen",
  "license": "MIT",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "bugs": {
    "url": "https://github.com/prek-no/intellipush-sdk/issues"
  },
  "homepage": "https://www.intellipush.com",
  "keywords": [
    "intellipush",
    "prek",
    "hirvi",
    "sms",
    "netron",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+git@github.com:prek-no/intellipush-sdk.git"
  },
  "files": [
    "dist/*.js",
    "dist/**/*.js",
    "dist/*.ts",
    "dist/**/*.ts"
  ],
  "scripts": {
    "test": "env TS_NODE_PROJECT=\"./config/tsconfig.cjs.json\" ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "clean": "rimraf dist/*",
    "build": "yarn clean && tsc --project config/tsconfig.esm.json & tsc --project config/tsconfig.cjs.json",
    "release": "yarn build && release-it",
    "release:beta": "yarn build && release-it --preRelease=beta",
    "release:rc": "yarn build && release-it --preRelease=rc"
  },
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/crypto-js": "^4.1.2",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jasmine": "^4.2.1",
    "release-it": "^15.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "isomorphic-unfetch": "^3.1.0",
    "joi": "^17.6.0"
  }
}
